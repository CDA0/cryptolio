{
  "name": "cryptolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "emulator": "/usr/local/share/android-sdk/tools/emulator -avd test -no-snapshot-load",
    "emulator:create": "avdmanager create avd -n test -k \"system-images;android-27;google_apis;x86\"",
    "emulator:run": "react-native run-android",
    "dev:ios": "react-native run-ios",
    "test": "jest",
    "test:update-snapshots": "jest -u",
    "test:watch": "jest --watch",
    "lint": "eslint src --fix",
    "precommit": "lint-staged && npm run test"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.52.2",
    "react-native-local-auth": "^1.5.0",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.0.3",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.1.0",
    "babel-preset-react-native": "4.0.0",
    "eslint": "^4.17.0",
    "eslint-config-semipretty": "^1.0.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-react": "^7.6.1",
    "husky": "^0.14.3",
    "jest": "22.1.4",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  }
}
